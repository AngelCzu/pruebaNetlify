{
    "nginx": {
      "static": {
        "serveFiles": true,
        "expires": "365d",
        "types": {
          "text/css": ["css"],
          "image/png": ["png"],
          "image/jpeg": ["jpg", "jpeg"],
          "image/gif": ["gif"],
          "application/javascript": ["js"]
        }
      },
      "location": "/",
      "proxy": ".cleverapps.io"
    }
  }